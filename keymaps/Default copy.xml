<keymap version="1" name="Default copy" parent="$default">
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt left" />
  </action>
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorCompleteStatement">
    <keyboard-shortcut first-keystroke="shift enter" />
  </action>
  <action id="EditorSelectWord" />
  <action id="EditorSplitLine" />
  <action id="EditorStartNewLine">
    <keyboard-shortcut first-keystroke="ctrl enter" />
  </action>
  <action id="EditorStartNewLineBefore">
    <keyboard-shortcut first-keystroke="shift ctrl enter" />
  </action>
  <action id="EditorUnSelectWord" />
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt right" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="ctrl alt k" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="RestoreDefaultLayout" />
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="shift f12" />
  </action>
  <action id="SurroundWith">
    <keyboard-shortcut first-keystroke="shift alt z" />
  </action>
  <action id="Terminal.OpenInTerminal">
    <keyboard-shortcut first-keystroke="shift alt f12" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="shift ctrl f11" />
  </action>
</keymap>